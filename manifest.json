{
  "manifest_version": 2,
  "name": "Open Weather Feed",
  "version": "1.0",

  "icons": {
    "64": "icons/icon-64.png"
  },

  "browser_action": {
    "default_icon": "icons/icon-64.png",
    "default_title": "Open Weather Feed",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
